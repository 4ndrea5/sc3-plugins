#! /bin/sh

svn export --force build Extensions
cp build/BEQSuiteUGens/*.scx Extensions/BEQSuiteUGens
cp build/BlackrainUGens/*.scx Extensions/BlackrainUGens
cp build/JoshUGens/*.scx Extensions/JoshUGens
cp build/ReverbUGens/*.scx Extensions/ReverbUGens
cp build/StkUGens/*.scx Extensions/StkUGens
cp build/MCLD_UGens/*.scx Extensions/MCLD_UGens
cp build/ljpclasses/*.scx Extensions/ljpclasses
cp build/bhobUGens/*.scx Extensions/bhobUGens
cp build/TagSystemUGens/*.scx Extensions/TagSystemUGens

mkdir Extensions/test
mv Extensions/StkUGens Extensions/test

