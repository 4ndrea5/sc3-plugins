#! /bin/sh

svn export --force build Extensions

mv AmbisonicUGens.so Extensions/AmbisonicUGens
mv BEQSuiteUGens.so Extensions/BEQSuiteUGens
mv BlackrainUGens.so Extensions/BlackrainUGens
mv JoshPVUGens.so Extensions/JoshPVUGens
