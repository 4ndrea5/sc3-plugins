
<faust>
	<name>JP_dreamverb_proto</name>
	<author>Julian Parker</author>
	<copyright>(c) Julian Parker 2013</copyright>
	<license>none</license>
	<version>1.0</version>
	<classname>mydsp</classname>
	<inputs>2</inputs>
	<outputs>2</outputs>
	<ui>
		<activewidgets>
			<count>12</count>
			<widget type="hslider" id="1">
				<label>damp</label>
				<varname>fslider11</varname>
				<init>0.0f</init>
				<min>0.0f</min>
				<max>0.999f</max>
				<step>0.0001f</step>
			</widget>
			<widget type="hslider" id="2">
				<label>earlyDiff</label>
				<varname>fslider1</varname>
				<init>0.707f</init>
				<min>0.0f</min>
				<max>0.99f</max>
				<step>0.001f</step>
			</widget>
			<widget type="hslider" id="3">
				<label>highBand</label>
				<varname>fslider5</varname>
				<init>2e+03f</init>
				<min>1e+03f</min>
				<max>1e+04f</max>
				<step>0.1f</step>
			</widget>
			<widget type="hslider" id="4">
				<label>highX</label>
				<varname>fslider10</varname>
				<init>1.0f</init>
				<min>0.0f</min>
				<max>1.0f</max>
				<step>0.01f</step>
			</widget>
			<widget type="hslider" id="5">
				<label>lowBand</label>
				<varname>fslider4</varname>
				<init>5e+02f</init>
				<min>1e+02f</min>
				<max>6e+03f</max>
				<step>0.1f</step>
			</widget>
			<widget type="hslider" id="6">
				<label>lowX</label>
				<varname>fslider9</varname>
				<init>1.0f</init>
				<min>0.0f</min>
				<max>1.0f</max>
				<step>0.01f</step>
			</widget>
			<widget type="hslider" id="7">
				<label>mDepth</label>
				<varname>fslider6</varname>
				<init>5.0f</init>
				<min>0.0f</min>
				<max>5e+01f</max>
				<step>0.01f</step>
			</widget>
			<widget type="hslider" id="8">
				<label>mFreq</label>
				<varname>fslider7</varname>
				<init>2.0f</init>
				<min>0.0f</min>
				<max>1e+01f</max>
				<step>0.01f</step>
			</widget>
			<widget type="hslider" id="9">
				<label>midX</label>
				<varname>fslider8</varname>
				<init>1.0f</init>
				<min>0.0f</min>
				<max>1.0f</max>
				<step>0.01f</step>
			</widget>
			<widget type="hslider" id="10">
				<label>size</label>
				<varname>fslider3</varname>
				<init>1.0f</init>
				<min>0.7f</min>
				<max>5.0f</max>
				<step>0.01f</step>
			</widget>
			<widget type="hslider" id="11">
				<label>t60</label>
				<varname>fslider2</varname>
				<init>1.0f</init>
				<min>0.1f</min>
				<max>6e+01f</max>
				<step>0.1f</step>
			</widget>
			<widget type="hslider" id="12">
				<label>wet</label>
				<varname>fslider0</varname>
				<init>0.5f</init>
				<min>0.0f</min>
				<max>1.0f</max>
				<step>0.01f</step>
			</widget>
		</activewidgets>
		
		<passivewidgets>
			<count>0</count>
		</passivewidgets>
		
		<layout>
			<group type="vgroup">
				<label>reverb</label>
				<widgetref id="1" />
				<widgetref id="2" />
				<widgetref id="3" />
				<widgetref id="4" />
				<widgetref id="5" />
				<widgetref id="6" />
				<widgetref id="7" />
				<widgetref id="8" />
				<widgetref id="9" />
				<widgetref id="10" />
				<widgetref id="11" />
				<widgetref id="12" />
			</group>
		</layout>
	</ui>
</faust>
